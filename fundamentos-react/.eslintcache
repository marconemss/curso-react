/git/curso-react/fundamentos-react/src/components/basicos/FamiliaMembro.jsx",["124"],"import React from 'react'\n\nexport default props => {\n    return <div> {props.nome} <strong>{props.sobrenome}</strong> </div>\n}\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/repeticao/ListaAlunos.jsx",["125"],"import React from 'react'\nimport alunos from '../../data/alunos'\n\nexport default props => {\n    const lis = alunos.map((aluno) => {\n        return (\n            /**\n             * Se não utilizar o key vai funcionar, \n             * mas o console do browser vai reclamar\n             */\n            <li key={aluno.id}>\n                {aluno.id} - {aluno.nome} : {aluno.nota}\n            </li>\n        )\n    })\n\n    return (\n         <div>\n             <ul style={ {listStyle: 'square'}}>\n                 {lis}\n             </ul>\n        </div>\n    )\n}","/home/marconemss/git/curso-react/fundamentos-react/src/data/alunos.js",["126"],"export default [\n    {id: 1, nome: 'Fulano', nota: 4.5},\n    {id: 2, nome: 'Beltrano', nota: 7.0},\n    {id: 3, nome: 'Ciclano', nota: 9.5},\n    {id: 4, nome: 'Joaquim', nota: 6.5}\n]","/home/marconemss/git/curso-react/fundamentos-react/src/components/repeticao/TabelaProdutos.jsx",["127"],"import React from 'react'\nimport Produtos from '../../data/produtos'\nimport './TabelaProdutos.css'\n\nexport default props => {\n\n    const produtos = Produtos.map( (produto, i) => {\n        return (\n            <tr key={produto.id}>\n                <td align=\"left\">\n                    {i+1} - {produto.nome}\n                </td>\n                <td align=\"right\">\n                    {\"R$ \"} {produto.preco}\n                </td>\n            </tr>\n        )\n    })\n\n    return (\n        <div>\n            <table border=\"0\" className=\"TabelaProdutos\">\n                <tbody>\n                    {produtos}\n                </tbody>\n            </table>\n        </div>\n    )\n}","/home/marconemss/git/curso-react/fundamentos-react/src/data/produtos.js",["128"],"export default [\n    {id: 1, nome: 'Mouse', preco: 20},\n    {id: 2, nome: 'Teclado', preco: 700},\n    {id: 3, nome: 'Monitor', preco: 1800},\n    {id: 4, nome: 'Gabinete', preco: 2000},\n    {id: 5, nome: 'Régua', preco: 150}\n]","/home/marconemss/git/curso-react/fundamentos-react/src/components/condicional/ParOuImpar.jsx",["129"],"import React from 'react'\n\nexport default props => {\n\n    /**\n     * O operador === compara não apenas o valor, mas o tipo, ex:\n     * \n     * 2 == '2' é true\n     * \n     * 2 === '2' é false\n     */\n    \n    const isPar = props.numero % 2 === 0;\n\n    return (\n        <div>\n            Número informado: {props.numero}\n            <p/>\n            {\n                isPar ? <span>Par</span>\n                : <span>Ímpar</span>\n            }\n        </div>\n    )\n}\n\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/condicional/UsuarioInfo.jsx",["130"],"import React from 'react'\nimport If, {Else} from './If'\n\nexport default props => {\n    const usuario = props.usuario || {}\n    return (\n        <div>\n            <If test={usuario && usuario.nome}>\n                Seja bem-vindo <strong>{ usuario.nome }</strong>\n                <Else>\n                    Seja bem-vindo Anônimo.\n                </Else>\n            </If>\n        </div>\n    )\n}\n\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/condicional/If.jsx",["131"],"export default props => {\n    /**\n     * Varre os filhos children;\n     * Para cada child encontrado, retorna aquele\n     * que tenha type e tenha nome === 'Else'\n     * \n     * O retorno disso tudo é um array, por isso\n     * eu pego apenas o primeiro elemento [0]\n     * levando-se em conta que o If só terá um único Else\n     */\n\n    const elseChild = props.children.filter(child => {\n        return child.type && child.type.name === 'Else'\n    })[0]\n\n    /**\n     * Vou pegar todos os elementos lá dentro da tag\n     * que não sejam o Else\n     */\n    const ifChildren = props.children.filter(child => {\n        return child !== elseChild;\n    })\n\n    if(props.test) {\n        return ifChildren;\n    } else if(elseChild) {\n        return elseChild;\n    } else {\n        return false;\n    }\n}\n\nexport const Else = props => props.children\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/comunicacao/DiretaPai.jsx",["132"],"import React from 'react'\nimport DiretaFilho from './DiretaFilho'\n\n\nexport default props => {\n    return (\n        <div>\n            <DiretaFilho texto=\"Filho1\" numero={20} bool={true} />\n        </div>\n    )\n}\n\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/comunicacao/DiretaFilho.jsx",["133"],"import React from 'react'\n\nexport default props => {\n    return (\n        <div>\n            <span>{props.texto} </span>\n            <span> <strong> {props.numero} </strong> </span>\n            <span>{props.bool ? 'V' : 'F'}</span>\n        </div>\n    )\n}\n\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/comunicacao/IndiretaPai.jsx",["134"],"/home/marconemss/git/curso-react/fundamentos-react/src/components/comunicacao/IndiretaFilho.jsx",["135"],"import React from 'react'\n\n\nexport default props => {\n\n    return (\n        <div>\n            <div>\n                Filho\n            </div>\n\n            <button onClick={\n                    function (e) {\n                        props.quandoClicar('Fulano', 40, false)\n                    }\n                    }>\n                Fornecer Informações\n            </button>\n            <br></br>\n            <button onClick={e => props.quandoClicar('Beltrano', 50, true)}>\n                Fornecer Informações com Função Arrow \n            </button>\n        </div>\n    )\n}\n\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/form