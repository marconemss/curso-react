 const elseChild = props.children.filter(child => {\n        return child.type && child.type.name === 'Else'\n    })[0]\n\n    /**\n     * Vou pegar todos os elementos lá dentro da tag\n     * que não sejam o Else\n     */\n    const ifChildren = props.children.filter(child => {\n        return child !== elseChild;\n    })\n\n    if(props.test) {\n        return ifChildren;\n    } else if(elseChild) {\n        return elseChild;\n    } else {\n        return false;\n    }\n}\n\nexport const Else = props => props.children\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/comunicacao/DiretaPai.jsx",["105"],"/home/marconemss/git/curso-react/fundamentos-react/src/components/comunicacao/DiretaFilho.jsx",["106"],"/home/marconemss/git/curso-react/fundamentos-react/src/components/comunicacao/IndiretaPai.jsx",["107"],"/home/marconemss/git/curso-react/fundamentos-react/src/components/comunicacao/IndiretaFilho.jsx",["108"],{"ruleId":"109","replacedBy":"110"},{"ruleId":"111","replacedBy":"112"},{"ruleId":"113","severity":1,"message":"114","line":4,"column":1,"nodeType":"115","endLine":19,"endColumn":2},{"ruleId":"113","severity":1,"message":"114","line":3,"column":1,"nodeType":"115","endLine":27,"endColumn":2},{"ruleId":"113","severity":1,"message":"114","line":3,"column":1,"nodeType":"115","endLine":5,"endColumn":2},{"ruleId":"113","severity":1,"message":"114","line":4,"column":1,"nodeType":"115","endLine":24,"endColumn":2},{"ruleId":"113","severity":1,"message":"116","line":1,"column":1,"nodeType":"115","endLine":6,"endColumn":2},{"ruleId":"113","severity":1,"message":"114","line":5,"column":1,"nodeType":"115","endLine":29,"endColumn":2},{"ruleId":"113","severity":1,"message":"116","line":1,"column":1,"nodeType":"115","endLine":7,"endColumn":2},{"ruleId":"113","severity":1,"message":"114","line":3,"column":1,"nodeType":"115","endLine":25,"endColumn":2},{"ruleId":"113","severity":1,"message":"114","line":4,"column":1,"nodeType":"115","endLine":16,"endColumn"