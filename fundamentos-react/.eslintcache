20","messages":"121","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"122","usedDeprecatedRules":"58"},{"filePath":"123","messages":"124","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"125","usedDeprecatedRules":"58"},{"filePath":"126","messages":"127","errorCount":0,"warningCount":2,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},"/home/marconemss/git/curso-react/fundamentos-react/src/index.js",[],["128","129"],"/home/marconemss/git/curso-react/fundamentos-react/src/components/basicos/Primeiro.js",[],"/home/marconemss/git/curso-react/fundamentos-react/src/components/basicos/ComParametro.jsx",[],"/home/marconemss/git/curso-react/fundamentos-react/src/components/basicos/Fragmento.jsx",[],"/home/marconemss/git/curso-react/fundamentos-react/src/App.jsx",[],"/home/marconemss/git/curso-react/fundamentos-react/src/components/basicos/Aleatorio.jsx",[],"/home/marconemss/git/curso-react/fundamentos-react/src/components/layout/Card.jsx",["130"],"import React from 'react'\nimport './Card.css'\n\nexport default props => {\n\n    const estilo = {\n        backgroundColor: props.color || '#10206B',\n        borderColor: props.borda || props.color\n    }\n\n    return (\n        <div className=\"Card\" style={estilo}>\n            <div className=\"Title\">{props.titulo}</div>\n            <div className=\"Content\">\n                {props.children}\n            </div>\n        </div>\n    )\n}\n\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/basicos/Familia.jsx",["131"],"import React, {cloneElement} from 'react'\n\nexport default props => {\n    return (\n        <div>\n            {\n                /** \n                 * React.Children.map :\n                 * -> Vai fazer um mapeamento nos filhos:\n                 * \n                 * props.children, child => \n                 * \n                 * -> para cada child de children, a funcao define que:\n                 * \n                 * cloneElement(child,props)\n                 * \n                 * -> clona o props para este child\n                 * \n                 * Repare que {cloneElement} precisa constar no import!\n                 */\n                React.Children.map(props.children, child => {\n                    return cloneElement(child,props)\n                })\n            }\n        </div>\n    )\n}\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/basicos/FamiliaMembro.jsx",["132"],"import React from 'react'\n\nexport default props => {\n    return <div> {props.nome} <strong>{props.sobrenome}</strong> </div>\n}\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/repeticao/ListaAlunos.jsx",["133"],"import React from 'react'\nimport alunos from '../../data/alunos'\n\nexport default props => {\n    const lis = alunos.map((aluno) => {\n        return (\n            /**\n             * Se não utilizar o key vai funcionar, \n             * mas o console do browser vai reclamar\n             */\n            <li key={aluno.id}>\n                {aluno.id} - {aluno.nome} : {aluno.nota}\n            </li>\n        )\n    })\n\n    return (\n         <div>\n             <ul style={ {listStyle: 'square'}}>\n                 {lis}\n             </ul>\n        </div>\n    )\n}","/home/marconemss/git/curso-react/fundamentos-react/src/data/alunos.js",["134"],"export default [\n    {id: 1, nome: 'Fulano', nota: 4.5},\n    {id: 2, nome: 'Beltrano', nota: 7.0},\n    {id: 3, nome: 'Ciclano', nota: 9.5},\n    {id: 4, nome: 'Joaquim', nota: 6.5}\n]","/home/marconemss/git/curso-react/fundamentos-react/src/components/repeticao/TabelaProdutos.jsx",["135"],"import React from 'react'\nimport Produtos from '../../data/produtos'\nimport './TabelaProdutos.css'\n\nexport default props => {\n\n    const produtos = Produtos.map( (produto, i) => {\n        return (\n            <tr key={produto.id}>\n                <td align=\"left\">\n                    {i+1} - {produto.nome}\n                </td>\n                <td align=\"right\">\n                    {\"R$ \"} {produto.preco}\n                </td>\n            </tr>\n        )\n    })\n\n    return (\n        <div>\n            <table border=\"0\" className=\"TabelaProdutos\">\n                <tbody>\n                    {produtos}\n                </tbody>\n            </table>\n        </div>\n    )\n}","/home/marconemss/git/curso-react/fundamentos-react/src/data/produtos.js",["136"],"export default [\n    {id: 1, nome: 'Mouse', preco: 20},\n    {id: 2, nome: 'Teclado', preco: 700},\n    {id: 3, nome: 'Monitor', preco: 1800},\n    {id: 4, nome: 'Gabinete', preco: 2000},\n    {id: 5, nome: 'Régua', preco: 150}\n]","/home/marconemss/git/curso-react/fundamentos-react/src/components/condicional/ParOuImpar.jsx",["137"],"import React from 'react'\n\nexport default props => {\n\n    /**\n     * O operador === compara não apenas o valor, mas o tipo, ex:\n     * \n     * 2 == '2' é true\n     * \n     * 2 === '2' é false\n     */\n    \n    const isPar = props.numero % 2 === 0;\n\n    return (\n        <div>\n            Número informado: {props.numero}\n            <p/>\n            {\n                isPar ? <span>Par</span>\n                : <span>Ímpar</span>\n            }\n        </div>\n    )\n}\n\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/condicional/UsuarioInfo.jsx",["138"],"import React from 'react'\nimport If, {Else} from './If'\n\nexport default props => {\n    const usuario = props.usuario || {}\n    return (\n        <div>\n            <If test={usuario && usuario.nome}>\n                Seja bem-vindo <strong>{ usuario.nome }</strong>\n                <Else>\n                    Seja bem-vindo Anônimo.\n                </Else>\n            </If>\n        </div>\n    )\n}\n\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/condicional/If.jsx",["139"],"export default props => {\n    /**\n     * Varre os filhos children;\n     * Para cada child encontrado, retorna aquele\n     * que tenha type e tenha nome === 'Else'\n     * \n     * O retorno disso tudo é um array, por isso\n     * eu pego apenas o primeiro elemento [0]\n     * levando-se em conta que o If só terá um único Else\n     */\n\n    const elseChild = props.children.filter(child => {\n        return child.type && child.type.name === 'Else'\n    })[0]\n\n    /**\n     * Vou pegar todos os elementos lá dentro da tag\n     * que não sejam o Else\n     */\n    const ifChildren = props.children.filter(child => {\n        return child !== elseChild;\n    })\n\n    if(props.test) {\n        return ifChildren;\n    } else if(elseChild) {\n        return elseChild;\n    } else {\n        return false;\n    }\n}\n\nexport const Else = props => props.children\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/comunicacao/DiretaPai.jsx",["140"],"import React from 'react'\nimport DiretaFilho from './DiretaFilho'\n\n\nexport default props => {\n    return (\n        <div>\n            <DiretaFilho texto=\"Filho1\" numero={20} bool={true} />\n        </div>\n    )\n}\n\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/comunicacao/DiretaFilho.jsx",["141"],"import React from 'react'\n\nexport default props => {\n    return (\n        <div>\n            <span>{props.texto} </span>\n            <span> <strong> {props.numero} </strong> </span>\n            <span>{props.bool ? 'V' : 'F'}</span>\n        </div>\n    )\n}\n\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/comunicacao/IndiretaPai.jsx",["142"],"import React, {useState} from 'react'\nimport IndiretaFilho from './IndiretaFilho'\n\n\nexport default props => {\n\n    //useState retorna um array: [valor, função]\n    \n    const [texto, setTexto] = useState('?')\n    const [numero, setNumero] = useState(0)\n    const [bool, setBool] = useState(false)\n\n    function fornecerInformacoes(textoParam, numeroParam, bolParam){\n        console.log(textoParam,numeroParam,bolParam)\n\n        setTexto(textoParam)\n        setNumero(numeroParam)\n        setBool(bolParam)\n\n        //Neste exemplo abaixo, apenas atribuir valor às variáveis não reflete \n        //nos span abaixo! Daí a necessidade de se utilizar o useState\n\n        //texto = textoParam;\n        //numero = numeroParam;\n        //bool = bolParam;\n    }\n\n    return (\n        <div>\n            <div>\n                <span>{texto} </span>\n                <span> <strong> {numero} </strong> </span>\n                <span>{bool ? 'V' : 'F'}</span>\n            </div>\n\n            <IndiretaFilho quandoClicar={fornecerInformacoes}/>\n        </div>\n    )\n}\n\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/comunicacao/IndiretaFilho.jsx",["143"],"import React from 'react'\n\n\nexport default props => {\n\n    return (\n        <div>\n            <div>\n                Filho\n            </div>\n\n            <button onClick={\n                    function (e) {\n                        props.quandoClicar('Fulano', 40, false)\n                    }\n                    }>\n                Fornecer Informações\n            </button>\n            <br></br>\n            <button onClick={e => props.quandoClicar('Beltrano', 50, true)}>\n                Fornecer Informações com Função Arrow \n            </button>\n        </div>\n    )\n}\n\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/formulario/Input.jsx",["144"],"/home/marconemss/git/curso-react/fundamentos-react/src/components/contador/Contador.jsx",[],"/home/marconemss/git/curso-react/fundamentos-react/src/components/contadorComponente/ContadorComponente.jsx",[],"/home/marconemss/git/curso-react/fundamentos-react/src/components/contadorComponente/Display.jsx",["145"],"import React from 'react'\n\nexport default props => {\n    return (\n        <h1>{props.numero}</h1>\n    )\n}\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/contadorComponente/Botoes.jsx",["146"],"import React from 'react'\n\nexport default props => {\n    return (\n        <div>\n            <button onClick={props.dec}>-</button>\n            <button onClick={props.inc}>+</button>\n        </div>\n    )\n}\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/contadorComponente/PassoForm.jsx",["147"],"import React from 'react'\n\nexport default props => {\n    return (\n\n        /**\n         * Sem implementar este onChange, o input não altera o seu valor!!\n         * \n         * Passa a ser um input readOnly!\n         */\n        <div>\n             <input type=\"number\" value={props.passo} \n                    onChange={e => props.doSetPasso(+e.target.value)}/>\n        </div>\n\n    )\n}\n\n","/home/marconemss/git/curso-react/fundamentos-react/src/components/mega/Mega.jsx",["148","149"],{"ruleId":"150","replacedBy":"151"},{"ruleId":"152","replacedBy":"153"},{"ruleId":"154","severity":1,"message":"155","line":4,"column":1,"nodeType":"156","endLine":19,"endColumn":2},{"ruleId":"154","severity":1,"message":"155","line":3,"column":1,"nodeType":"156","endLine":27,"endColumn":2},{"ruleId":"154","severity":1,"message":"155","line":3,"column":1,"nodeType":"156","endLine":5,"endColumn":2},{"ruleId":"154","severity":1,"message":"155","line":4,"column":1,"nodeType":"156","endLine":24,"endColumn":2},{"ruleId":"154","severity":1,"message":"157","line":1,"column":1,"nodeType":"156","endLine":6,"endColumn":2},{"ruleId":"154","severity":1,"message":"155","line":5,"column":1,"nodeType":"156","endLine":29,"endColumn":2},{"ruleId":"154","severity":1,"message":"157","line":1,"column":1,"nodeType":"156","endLine":7,"endColumn":2},{"ruleId":"154","severity":1,"message":"155","line":3,"column":1,"nodeType":"156","endLine":25,"endColumn":2},{"ruleId":"154","severity":1,"message":"155","line":4,"column":1,"nodeType":"156","endLine":16,"endCo